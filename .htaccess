RewriteEngine on

RewriteRule ^anasayfa(\.html)$ index.php [L,NC]
RewriteRule ^index(\.html)$ index.php [L,NC]
RewriteRule ^iletisim(\.html)$ iletisim.php [L,NC]
RewriteRule ^arama(\.html)$ arama.php [L,NC]
RewriteRule ^kategori-detay-([a-zA-Z0-9\-_]+)?(\.html)$ kategoridetay.php?id=$1 [L,QSA]
RewriteRule ^haberler(\.html)$ haberler.php [L,NC]
RewriteRule ^hizmetler(\.html)$ hizmetler.php [L,NC]
RewriteRule ^hizmet-detay-([a-zA-Z0-9\-_]+)?(\.html)$ hizmetdetay.php?id=$1 [L,QSA]
RewriteRule ^haber-detay-([a-zA-Z0-9\-_]+)?(\.html)$ haberdetay.php?id=$1 [L,QSA]
RewriteRule ^sayfa-detay-([a-zA-Z0-9\-_]+)?(\.html)$ sayfadetay.php?id=$1 [L,QSA]
RewriteRule ^foto-galeri(\.html)$ fotogaleri.php [L,NC]
RewriteRule ^projelerimiz(\.html)$ projelerimiz.php [L,NC]
RewriteRule ^proje-detay-([a-zA-Z0-9\-_]+)?(\.html)$ projedetay.php?id=$1 [L,QSA]
RewriteRule ^oda-detay-([a-zA-Z0-9\-_]+)?(\.html)$ odadetay.php?id=$1 [L,QSA]
RewriteRule ^rez-detay-([a-zA-Z0-9\-_]+)?(\.html)$ rezdetay.php?id=$1 [L,QSA]
RewriteRule ^odalar(\.html)$ odalar.php [L,NC]
RewriteRule ^rezervasyon(\.html)$ odalar.php [L,NC]
RewriteRule ^z-defteri(\.html)$ zdefteri.php [L,NC]
RewriteRule ^restorant(\.html)$ restorant.php [L,NC]
RewriteRule ^mesaj(\.html)$ mesaj.php [L,NC]
RewriteRule ^portfolyo(\.html)$ portfolyo.php [L,NC]
RewriteRule ^calisma-ekibimiz(\.html)$ ekibimiz.php [L,NC]